        <description/>
        <name>GetEmailAddressAJAX</name>
        <script><![CDATA[var GetEmailAddressAJAX = Class.create();
GetEmailAddressAJAX.prototype = Object.extendsObject(global.AbstractAjaxProcessor, {
	
	getEmail: function(){
		var reqForSysId = this.getParameter('sysparm_userId');
		
		var grUser = new GlideRecord('sys_user');
		if(grUser.get(reqForSysId))
			return grUser.email + '';
		else
			return '';
	},

    type: 'GetEmailAddressAJAX'
});]]></script>
